_[/* c */ \"d\"])(this, \"celSingleSelect\", 7);\n    this.itemEntered = Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* c */ \"d\"])(this, \"singleSelectItemClicked\", 7);\n    this.openSingleSelectDropdown = Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* c */ \"d\"])(this, \"celSingleSelectOpenDropdown\", 7);\n    this.blurSingleSelectDropdown = Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* c */ \"d\"])(this, \"celSingleSelectBlurDropdown\", 7);\n    this.singleSelectId = `cel-single-select-${singleSelectDropdownIds++}`;\n    this.validDefaultItem = false;\n    this.selectedItem = undefined;\n    this.selected = undefined;\n    this.labelLeft = false;\n    this.primary = false;\n    this.secondaryLabel = undefined;\n    this.placeholder = 'Select Placeholder';\n    this.singleSelectAriaLabel = 'Select an option';\n    this.disabled = false;\n    this.menuList = [];\n    this.caretSize = _cel_icon_constants_33baae7b_js__WEBPACK_IMPORTED_MODULE_3__[/* a */ \"b\"].MEDIUM;\n    this.isRequired = false;\n    this.caretSrc = undefined;\n    this.titleCharLength = 18;\n    this.enableError = false;\n    this.enableCustom = false;\n    this.statusConfig = undefined;\n    this.enableSuccess = false;\n    this.lstTable = false;\n    this.none = 'None';\n    this.toggleValueReset = false;\n    this.focusElement = false;\n  }\n  handleValueReset() {\n    this.selectedItem = null;\n    this.selectEl.selectedIndex = -1;\n    this.menuList.forEach((menuItem) => {\n      if (menuItem.identifier == this.selected) {\n        this.selectedItem = menuItem;\n        this.selectEl.selectedIndex = this.menuList.indexOf(menuItem);\n        return;\n      }\n    });\n  }\n  updateSelectedItem() {\n    this.menuList.forEach((menuItem) => {\n      if (menuItem.identifier == this.selected) {\n        this.selectedItem = menuItem;\n        return;\n      }\n    });\n    this.validDefaultItem = !!this.selectedItem;\n  }\n  resizeSelectTag() {\n    if (!this.pri