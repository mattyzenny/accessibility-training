  emitCheckValueChangeEvent(targetValue) {\n    if (this.handleCheckedExternally) {\n      this.inputChangeAttempted.emit({ identifier: this.identifier, checked: targetValue, checkboxType: this.checkboxType });\n    }\n    else if (this.checkboxType) {\n      this.headerInputChanged.emit({ identifier: this.identifier, checked: targetValue, checkboxType: this.checkboxType });\n    }\n    else {\n      this.inputChanged.emit({ identifier: this.identifier, checked: targetValue, checkboxType: this.checkboxType });\n    }\n  }\n  componentDidLoad() {\n    this.setTitleAttributeByOverflow(this.labelEl);\n    if (this.focusElement && this.inputEl) {\n      this.inputEl.focus();\n    }\n  }\n  handleResize() {\n    this.setTitleAttributeByOverflow(this.labelEl);\n  }\n  componentDidUpdate() {\n    this.setTitleAttributeByOverflow(this.labelEl);\n  }\n  setTitleAttributeByOverflow(element) {\n    if (element) {\n      if (element.scrollHeight > element.clientHeight || element.scrollWidth > element.clientWidth) {\n        element.setAttribute('title', element.textContent);\n        element.removeAttribute('aria-hidden');\n      }\n      else {\n        element.removeAttribute('title');\n        // if there is a predefined aria-label OR the text is truncated, set aria-hidden to false bc we are relying on the \n        // aria-label on the input and the label here is redundant/ announced x2\n        this.checkboxItemAriaLabel ? element.setAttribute('aria-hidden', 'true') : element.removeAttribute('aria-hidden');\n      }\n    }\n  }\n  handleFocusElement() {\n    if (this.focusElement && this.inputEl) {\n      this.inputEl.focus();\n    }\n  }\n  render() {\n    return (Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* h */ \"g\"])(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* H */ \"a\"], { \"data-cel-version\": '10.1.4', \"data-identifier\": this.identifier,\n      // using inline style since adding a class to host did not work and using :has() in host did not wo