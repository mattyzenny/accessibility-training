it !important;font-style:inherit !important;font-family:OpenSans !important}.mq-editable-field{font-family:var(--editor-font-family, OpenSans);font-size:var(--editor-font-size, 1rem);color:#333}.no-decoration{text-decoration:none !important;color:#333 !important}.mq-math-mode .mq-scaled{display:-ms-inline-flexbox;display:inline-flex;height:-webkit-fit-content;height:-moz-fit-content;height:fit-content}\";\n\nconst MQ = MathEditor.getInterface();\nconst CelRichTextMathEditor = class {\n  constructor(hostRef) {\n    registerInstance(this, hostRef);\n    this.componentLoaded = createEvent(this, \"celRichTextMathEditorLoaded\", 7);\n    this.activeMathEditorStatus = createEvent(this, \"celRichTextActiveMathEditorStatus\", 7);\n    this.mathEmbedFocusChange = createEvent(this, \"celRichTextMathEmbedFocusChange\", 7);\n    this.isMathModeChanged = false;\n    this.enableMobileView = undefined;\n    this.latex = '';\n    this.previousLatex = '';\n    this.celIconBaseUrl = '';\n    this.mathEditorMode = 'viewer';\n    this.enableMathEditor = false;\n    this.mathEditorFocusMode = '';\n    this.celButtonTagName = 'cel-button';\n    this.mathKeypadTitle = '';\n    this.stickKeypadToBottom = false;\n    this.dragIconAriaLabel = '';\n    this.keypadLeftArrowDisabledLabel = '';\n    this.keypadRightArrowDisabledLabel = '';\n  }\n  handleModeChange() {\n    this.isMathModeChanged = true;\n  }\n  handleFocusModeChange() {\n    switch (this.mathEditorFocusMode) {\n      case 'focusin':\n        this.componentEl.classList.add('highlight-editor');\n        this.convertedMathML = this.getConvertedMathML(this.latex);\n        this.updateMathSpeechOutput();\n        break;\n      case 'focusout':\n        this.componentEl.classList.remove('highlight-editor');\n        this.mathEmbedFocusChange.emit('');\n        break;\n      //DO NOTHING\n    }\n  }\n  handleEditorStatus() {\n    if (this.enableMathEditor && (this.mathEditorMode === 'viewer' || this.mathEditorMode === 'load')) {\n      this.componentEl.setAttribute('data-math-editor-mode', 'editor');\n    }\n  }\n  onMathKeypadClose(eventObj) {\n    const { detail: { latex, origin } } = eventObj;\n    if (latex) {\n      this.componentEl.setAttribute('data-latex', latex);\n      this.componentEl.setAttribute('data-previous-latex', this.latex);\n    }\n    else if (this.previousLatex && origin !== 'save') {\n      this.componentEl.setAttribute('data-latex', this.previousLatex);\n    }\n    else {\n      this.componentEl.setAttribute('data-latex', '');\n    }\n    this.switchEditorModeToViewer();\n  }\n  closeMathPalette() {\n    this.switchEditorModeToViewer();\n  }\n  handleCelDraggableWindowLoaded() {\n    this.componentLoaded.emit(true);\n  }\n  handleMobileView(eventObj) {\n    this.enableMobileView = eventObj.detail;\n  }\n  connectedCallback() {\n    this.cel_rich_text_math_keypad = transformTagName('cel-rich-text-math-keypad', 'cel-rich-text-math-editor', this.el);\n    this.cel_draggable_window = transformTagName('cel-draggable-window', 'cel-rich-text-math-editor', this.el);\n    this.cel_rich_text_math_expression_viewer = transformTagName('cel-rich-text-math-expression-viewer', 'cel-rich-text-math-editor', this.el);\n  }\n  componentDidLoad() {\n    this.toggleMathEditor();\n  }\n  componentWillUpdate() {\n    if (this.isMathModeChanged) {\n      setTimeout(() => {\n        this.toggleMathEditor();\n      });\n      this.isMathModeChanged = false;\n    }\n  }\n  toggleMathEditor() {\n    if (this.componentEl) {\n      switch (this.mathEditorMode) {\n        case 'editor':\n          this.loadActiveMathEditor();\n          break;\n        case 'viewer':\n        case 'load':\n          this.loadStaticMathEditor();\n          break;\n        //DO NOTHING\n      }\n      if (!this.enableMathEditor) {\n        this.componentEl.setAttribute('contenteditable', 'false');\n      }\n    }\n  }\n  getConvertedMathML(latex) {\n    return MathEditor.convertLatexToMathML(latex);\n  }\n  loadActiveMathEditor() {\n    var _a;\n    if (this.mathEditorMode !== 'load')\n      this.activeMathEditorStatus.emit(true);\n    const mathEditorContainer = this.componentEl.querySelector('.cel-rich-text-math-container');\n    let mathEditor = mathEditorContainer.querySelector('.cel-rich-text-math-edit-container');\n    mathEditor.innerHTML = this.latex;\n    this.mathFieldEditor = MQ.StaticMath(mathEditor);\n    (_a = this.mathFieldEditor) === null || _a === void 0 ? void 0 : _a.latex(this.latex);\n  }\n  loadStaticMathEditor() {\n    if (this.mathEditorMode !== 'load')\n      this.activeMathEditorStatus.emit(false);\n    if (!this.latex) {\n      this.componentEl.innerHTML = '';\n    }\n  }\n  switchEditorModeToViewer() {\n    this.componentEl.setAttribute('data-math-editor-mode', 'viewer');\n  }\n  //Update speech output - which will announce in screen reader\n  updateMathSpeechOutput() {\n    if (this.convertedMathML.katexError) {\n      this.mathEmbedFocusChange.emit(this.convertedMathML.mathSpeechOutput);\n    }\n    else {\n      this.mathEmbedFocusChange.emit('Math expression ' + this.convertedMathML.mathSpeechOutput);\n    }\n  }\n  render() {\n    return (h(Host, { ref: (el) => (this.componentEl = el) }, h(\"span\", { class: 'cel-rich-text-math-container' }, this.mathEditorMode === 'editor' ? (h(\"span\", { class: 'cel-rich-text-math-container-holder' }, h(\"span\", { class: 'cel-rich-text-math-edit-container', innerHTML: this.latex }), h(this.cel_draggable_window, { \"data-show-component-load-info\": 'true', \"data-cel-icon-base-url\": this.celIconBaseUrl, \"data-last-focusable-element\": this.celButtonTagName, \"data-title-text\": this.mathKeypadTitle, \"data-last-focusable-element-identifier\": '#Insert', \"data-drag-icon-aria-label\": this.dragIconAriaLabel, \"data-stick-modal-to-bottom\": this.stickKeypadToBottom }, h(this.cel_rich_text_math_keypad, { class: 'math-palette-body', \"data-latex\": this.latex, \"data-cel-icon-base-url\": this.celIconBaseUrl, \"data-keypad-mobile-view\": this.enableMobileView, \"data-keypad-left-arrow-disabled-label\": this.keypadLeftArrowDisabledLabel, \"data-keypad-right-arrow-disabled-label\": this.keypadRightArrowDisabledLabel })))) : null, this.mathEditorMode !== 'editor' && this.latex ?\n      (h(this.cel_rich_text_math_expression_viewer, { \"data-latex\": this.latex })) : null)));\n  }\n  get el() { return getElement(this); }\n  static get watchers() { return {\n    \"mathEditorMode\": [\"handleModeChange\"],\n    \"mathEditorFocusMode\": [\"handleFocusModeChange\"]\n  }; }\n};\nCelRichTextMathEditor.style = celRichTextMathEditorCss;\n\nexport { CelRichTextMathEditor as cel_rich_text_math_editor };\n"]}}