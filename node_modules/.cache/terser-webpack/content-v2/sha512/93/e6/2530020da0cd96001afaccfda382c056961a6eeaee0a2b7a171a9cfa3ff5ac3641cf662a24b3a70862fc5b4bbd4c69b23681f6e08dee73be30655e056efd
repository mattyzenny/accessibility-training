 else {\n      let childElement;\n      if (siblingElement === 'previousSibling') {\n        childElement = 'lastChild';\n      }\n      else {\n        childElement = 'firstChild';\n      }\n      const targetElement = currentActiveItem\n        .parentElement[childElement];\n      this.onFocusItem(targetElement);\n    }\n  }\n  onHandleCloseEvent() {\n    this.closeEvent.emit({ isClose: true, parentTargetElement: this.parentTargetElement });\n  }\n  makeFirstItemFocused() {\n    var _a;\n    const itemsElementList = (_a = this.el.shadowRoot) === null || _a === void 0 ? void 0 : _a.querySelectorAll(`.${this.itemClassName}`);\n    if (itemsElementList === null || itemsElementList === void 0 ? void 0 : itemsElementList.length) {\n      const firstFocusableElement = itemsElementList[0];\n      this.onFocusItem(firstFocusableElement);\n    }\n  }\n  showTitleOnLabelTruncation(label) {\n    return this.truncateLength && label.length > this.truncateLength ? label : null;\n  }\n  onItemClick(index, event = null) {\n    if (event) {\n      event.preventDefault();\n    }\n    let selectedItem;\n    this.items = this.items.map((item, i) => {\n      if (i === index) {\n        item.ariaChecked = 'true';\n        selectedItem = item;\n      }\n      else if (item.ariaChecked === 'true') {\n        item.ariaChecked = 'false';\n      }\n      return item;\n    });\n    this.itemSelected.emit(selectedItem);\n  }\n  onFocusItem(itemToFocus) {\n    if (itemToFocus) {\n      itemToFocus.tabIndex = 0;\n      itemToFocus.focus();\n    }\n  }\n  render() {\n    return (h(Host, { \"data-cel-version\": '9.0.2' }, h(\"ul\", { role: 'menu', \"aria-label\": this.menuAriaLabel, class: 'radio-menu-container' }, this.items.map((item, i) => {\n      return (h(\"li\", { role: 'menuitemradio', id: `${item.id}-radio-menu-item-${i}`, class: this.itemClassName, title: this.showTitleOnLabelTruncation(item.label), \"aria-checked\": item.ariaChecked, onClick: () => this.onItemClick(i) }, h(\"span\", { class: 'selected-icon-container' }, h(this.cel_icon, { \"aria-hidden\": 'true', class: item.ariaChecked === 'true' ? 'show' : 'hide', \"data-name\": 'checkmark', \"data-size\": CelIconSizes.XSMALL, \"data-color\": CelIconColors.GRAYDARKER })), item.iconName ? (h(this.cel_icon, { \"aria-hidden\": 'true', class: 'item-icon', \"data-name\": item.iconName })) : item.imgSrc ? (h(\"img\", { class: 'item-img', src: item.imgSrc, alt: item.imgSrcAltText })) : null, truncate(item.label, this.truncateLength)));\n    }))));\n  }\n  get el() { return getElement(this); }\n};\nCelRadioMenu.style = celRadioMenuCss;\n\nexport { CelRadioMenu as cel_radio_menu };\n"]}}