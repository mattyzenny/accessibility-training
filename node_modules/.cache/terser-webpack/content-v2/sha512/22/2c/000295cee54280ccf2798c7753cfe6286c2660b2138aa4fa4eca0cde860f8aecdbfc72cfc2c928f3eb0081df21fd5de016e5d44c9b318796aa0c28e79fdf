n        event.preventDefault();\n        (_b = (_a = nextControl === null || nextControl === void 0 ? void 0 : nextControl.shadowRoot) === null || _a === void 0 ? void 0 : _a.querySelector('button')) === null || _b === void 0 ? void 0 : _b.focus();\n        break;\n      case ARROW_LEFT:\n      case ARROW_UP:\n        const previousControl = allFocusableElements[previousIndex];\n        event.preventDefault();\n        (_d = (_c = previousControl === null || previousControl === void 0 ? void 0 : previousControl.shadowRoot) === null || _c === void 0 ? void 0 : _c.querySelector('button')) === null || _d === void 0 ? void 0 : _d.focus();\n        break;\n      //DO NOTHING\n    }\n  }\n  render() {\n    return (h(Host, { \"data-cel-version\": '9.0.2' }, h(\"div\", { class: \"sc-quill-cel-rich-text-toolbar-container\", role: \"toolbar\", \"aria-orientation\": \"horizontal\" }, h(\"div\", { class: \"cel-visually-hidden\", role: \"alert\" }, this.toolbarSelectedText), h(\"div\", { class: \"sc-quill-cel-rich-text-inner-container\", ref: (el) => (this.toolbarContainer = el) }, this.toolbarItemList.map((itemGroup, groupIndex) => Object.keys(itemGroup).map((groupType) => (itemGroup[groupType].map((iconType, iconIndex) => (h(this.cel_rich_text_toolbar_icon_button, { celRteEditorStore: this.celRteEditorStore, celRteToolbarStore: this.celRteToolbarStore, \"data-rte-icon-group-type\": groupType, \"data-rte-icon-type\": iconType, \"data-cel-icon-src\": this.celIconBaseUrl, \"data-cel-icon-padding\": this.buttonPadding, \"data-add-icon-group-separator\": ((this.toolbarItemList.length !== (groupIndex + 1)) && (itemGroup[groupType].length === (iconIndex + 1))) ? true : false }))))))))));\n  }\n  get el() { return getElement(this); }\n};\nCelRichTextToolbar.style = celRichTextToolbarCss;\n\nexport { CelRichTextToolbar as cel_rich_text_toolbar };\n"]}}