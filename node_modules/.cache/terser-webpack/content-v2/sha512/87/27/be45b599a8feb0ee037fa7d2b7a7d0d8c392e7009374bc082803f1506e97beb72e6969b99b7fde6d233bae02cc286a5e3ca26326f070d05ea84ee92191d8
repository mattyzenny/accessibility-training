-align:left;padding-right:8px}\";\n\nconst CelAccordionItem = class {\n  constructor(hostRef) {\n    registerInstance(this, hostRef);\n    this.pressEvent = createEvent(this, \"celAccordionItemPress\", 7);\n    this.accordionItemHeaderIconClicked = createEvent(this, \"celAccordionItemHeaderIconClick\", 7);\n    this.accordionContentId = `cel-accordionItem-${++accordionItemIds}`;\n    this.accordionBtnId = `cel-accordionBtn-${++accordionBtnIds}`;\n    this.hovered = false;\n    this.focused = false;\n    this.identifier = undefined;\n    this.index = undefined;\n    this.mobile = undefined;\n    this.caretRight = false;\n    this.pressed = false;\n    this.lastItem = undefined;\n    this.headerLabel = undefined;\n    this.contentText = undefined;\n    this.secondaryHeader = undefined;\n    this.secondaryHeaderTextLabel = undefined;\n    this.secondaryHeaderTextLabelCustomFillColor = undefined;\n    this.secondaryHeaderTextLabelCustomFontColor = undefined;\n    this.secondaryHeaderTextLabelFillColor = DataVisualizationColorKeys.WHITE;\n    this.secondaryHeaderIconLabel = undefined;\n    this.secondaryHeaderIconName = 'information-circle';\n    this.secondaryHeaderIconColor = CelIconColors.BLACK;\n    this.secondaryHeaderIconSize = CelIconSizes.REGULAR;\n    this.secondaryHeaderIconAriaLabel = undefined;\n    this.secondaryHeaderIconDisabled = undefined;\n  }\n  connectedCallback() {\n    this.cel_icon = transformTagName('cel-icon', 'cel-accordion-item', this.el);\n    this.cel_icon_button = transformTagName('cel-icon-button', 'cel-accordion-item', this.el);\n  }\n  itemPressed(event) {\n    const { className } = event.target;\n    // do not fire the event if the user clicks the icon button, only the accordion header itself\n    if (className.includes('secondary-header-icon-button')) {\n      return;\n    }\n    else {\n      this.pressed = !this.pressed;\n      this.pressEvent.emit({ identifier: this.identifier, pressed: this.pressed, headerLabel: this.headerLabel });\n    }\n  }\n  itemMouseEnter(event) {\n    const { className } = event.target;\n    // do not fire the event if the user hovers the icon button, only the accordion header itself\n    if (className.includes('secondary-header-icon-button')) {\n      return;\n    }\n    else {\n      this.hovered = true;\n      this.pressed ? this.heading.setAttribute('title', this.headerLabel) : null;\n    }\n  }\n  itemMouseLeave(event) {\n    const { className } = event.target;\n    if (className.includes('secondary-header-icon-button')) {\n      return;\n    }\n    else {\n      this.hovered = false;\n    }\n  }\n  componentWillLoad() {\n    this.hasHeaderSlot = !!this.el.querySelector('[slot=\"header\"]');\n  }\n  componentDidLoad() {\n    this.headerButtons = this.el.shadowRoot.querySelectorAll('.item-heading');\n  }\n  componentDidUpdate() {\n    setTitleAttributeByOverflow(this.headerButtons);\n  }\n  handleHeaderIconButtonClick(event) {\n    const { detail } = event;\n    if (detail.buttonType === 'accordion') {\n      const accordionEmitObject = {\n        accordionItemId: this.identifier,\n        accordionItemHeaderLabel: this.headerLabel,\n        accordionItemIndex: this.index,\n        ...detail\n      };\n      this.accordionItemHeaderIconClicked.emit(accordionEmitObject);\n    }\n    else {\n      return;\n    }\n  }\n  render() {\n    var _a;\n    return (h(Host, { \"data-cel-version\": '9.0.2' }, h(\"div\", { class: ['accordion-item', this.mobile ? 'accordion-item--mobile' : '', !this.lastItem ? 'border-bottom' : ''].join(' ') }, h(\"h2\", { onClick: (event) => this.itemPressed(event), onMouseEnter: (event) => this.itemMouseEnter(event), onMouseLeave: (event) => this.itemMouseLeave(event), ref: (el) => (this.heading = el), class: `accordion-header ${this.caretRight && !this.secondaryHeader ? 'header-caret-right' : ''}` }, h(\"button\", { class: [\n        this.hasHeaderSlot ? 'slot-caret' : '',\n        this.hovered ? 'accordion-item--hover' : '',\n        this.focused ? 'accordion-item--focus' : '',\n        this.caretRight && !this.secondaryHeader ? 'caret-right' : '',\n        this.secondaryHeader ? 'secondary-header-button' : '',\n        'header-button'\n      ].join(' '), \"aria-expanded\": this.pressed ? 'true' : 'false', ref: (el) => (this.buttonEl = el), onFocus: () => (this.focused = true), onBlur: () => (this.focused = false), id: this.accordionBtnId, \"aria-controls\": this.accordionBtnId }, !this.caretRight || this.secondaryHeader ? (h(this.cel_icon, { \"aria-hidden\": 'true', \"data-size\": 'regular', class: 'item-icon--left', \"data-name\": this.pressed ? 'caret-down' : 'caret-right', \"data-color\": this.hovered || this.focused ? CelIconColors.PRIMARY_SHADE : CelIconColors.BLACK })) : null, !this.hasHeaderSlot ? (h(\"span\", { class: ['item-heading', this.hovered || this.focused ? 'item-heading--focus' : '', this.pressed ? 'heading-expanded' : 'heading-collapsed'].join(' ') }, this.headerLabel)) : h(\"span\", { class: ['screen-readers-only'].join(' ') }, this.headerLabel), this.caretRight && !this.secondaryHeader ? (h(this.cel_icon, { \"aria-hidden\": 'true', class: 'item-icon--right', \"data-name\": this.pressed ? 'caret-up' : 'caret-down', \"data-color\": this.hovered || this.focused ? CelIconColors.PRIMARY_SHADE : CelIconColors.BLACK })) : null), this.secondaryHeader ? (h(\"div\", { class: 'secondary-header-content' }, h(\"p\", { class: `secondary-header-text-label ${this.secondaryHeaderTextLabelFillColor}`, style: {\n        color: this.secondaryHeaderTextLabelFillColor === 'custom' ? this.secondaryHeaderTextLabelCustomFontColor : null,\n        backgroundColor: this.secondaryHeaderTextLabelFillColor === 'custom' ? this.secondaryHeaderTextLabelCustomFillColor : 'none'\n      } }, this.secondaryHeaderTextLabel), h(\"div\", { class: 'secondary-header-icon-content' }, h(\"p\", { class: 'secondary-header-icon-label' }, this.secondaryHeaderIconLabel), h(this.cel_icon_button, { class: 'secondary-header-icon-button', \"data-identifier\": `${this.identifier}-icon-button`, \"data-name\": this.secondaryHeaderIconName, \"data-icon-button-aria-label\": this.secondaryHeaderIconAriaLabel, \"data-color\": (_a = this.secondaryHeaderIconColor) !== null && _a !== void 0 ? _a : CelIconColors.GRAYDARKER, \"data-size\": this.secondaryHeaderIconSize, \"data-disabled\": this.secondaryHeaderIconDisabled, \"data-button-type\": 'accordion' })))) : null, this.hasHeaderSlot ? (h(\"slot\", { name: \"header\" })) : null), h(\"div\", { id: this.accordionContentId, role: 'region', \"aria-labelledby\": this.accordionBtnId }, this.pressed && this.contentText ? (h(\"p\", { class: ['item-content', this.caretRight && !this.secondaryHeader ? 'item-content-left' : ''].join(' ') }, this.contentText)) : null, this.pressed ? h(\"slot\", null) : ''))));\n  }\n  get el() { return getElement(this); }\n};\nlet accordionItemIds = 0;\nlet accordionBtnIds = 0;\nCelAccordionItem.style = celAccordionItemCss;\n\nexport { CelAccordionItem as cel_accordion_item };\n"]}}