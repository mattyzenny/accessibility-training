ateButtonsBack\", 7);\n    this.numbersOnlyPattern = /^[0-9]+$/;\n    this.errorMessage = undefined;\n    this.label = undefined;\n    this.currentPage = undefined;\n    this.totalPages = undefined;\n    this.backButtonLabel = 'Back';\n    this.nextButtonLabel = 'Next';\n    this.paginationAriaLabel = 'Page navigation controls';\n    this.ofLabel = 'of';\n    this.inputErrorMessage = 'Numbers Only';\n  }\n  handePageChange(newPage) {\n    if (newPage > this.totalPages) {\n      this.currentPage = this.totalPages;\n    }\n    if (newPage < 1) {\n      this.currentPage = 1;\n    }\n    this.pageChange.emit(newPage);\n  }\n  connectedCallback() {\n    this.cel_icon = transformTagName('cel-icon', 'cel-paginate-buttons', this.el);\n    this.cel_button = transformTagName('cel-button', 'cel-paginate-buttons', this.el);\n  }\n  componentWillLoad() {\n    this.handePageChange(this.currentPage);\n  }\n  handleKeydown(ev) {\n    if (ev.key === ENTER) {\n      const input = this.el.shadowRoot.querySelector('.page-field');\n      const rawValue = input.value;\n      if (!rawValue.match(this.numbersOnlyPattern)) {\n        this.errorMessage = this.inputErrorMessage;\n        this.celSubmit.emit(rawValue);\n        return;\n      }\n      let value = parseInt(rawValue);\n      this.errorMessage = '';\n      if (value > this.totalPages) {\n        value = this.totalPages;\n      }\n      if (value < 1) {\n        value = 1;\n      }\n      const val = value;\n      this.currentPage = val;\n      this.celSubmit.emit(val);\n    }\n  }\n  render() {\n    return (h(Host, { \"data-cel-version\": '9.0.2' }, h(\"nav\", null, h(\"ul\", { class: \"paginate-controls\", \"aria-label\": this.paginationAriaLabel }, h(\"li\", { class: \"input-container\" }, h(\"label\", null, this.label, h(\"input\", { class: \"page-field\", type: \"quantity\", value: this.currentPage, onKeyDown: (ev) => this.handleKeydown(ev), \"aria-invalid\": this.errorMessage ? 'true' : 'false', \"aria-describedby\": this.errorMessage ? 'input-error' : null })), h(\"span\", { class: \"delimiter\" }, this.ofLabel, ' '), h(\"span\", { class: \"max-pages\" }, this.totalPages), this.errorMessage ? (h(\"div\", { class: \"error-container\" }, h(this.cel_icon, { class: \"error-icon\", \"data-name\": 'alert-circle', \"data-size\": CelIconSizes.SMALL, \"data-color\": CelIconColors.ERROR, \"aria-hidden\": \"true\" }), h(\"span\", { id: \"input-error\", class: \"error-message\" }, this.errorMessage))) : null), h(\"li\", { class: \"buttons-wrapper\" }, h(\"ul\", null, h(\"li\", null, h(this.cel_button, { class: \"back\", \"data-variant\": \"outline\", \"data-color\": CelIconColors.PRIMARY, \"data-icon-type\": \"left\", \"data-label\": this.backButtonLabel, \"data-name\": \"caret-left\", \"data-disabled\": this.currentPage <= 1, onClick: (ev) => this.celBack.emit(ev) })), h(\"li\", null, h(this.cel_button, { class: \"next\", \"data-variant\": \"solid\", \"data-color\": CelIconColors.WHITE, \"data-icon-type\": \"right\", \"data-label\": this.nextButtonLabel, \"data-name\": \"caret-right\", \"data-disabled\": this.currentPage >= this.totalPages, onClick: (ev) => this.celNext.emit(ev) }))))))));\n  }\n  get el() { return getElement(this); }\n  static get watchers() { return {\n    \"currentPage\": [\"handePageChange\"]\n  }; }\n};\nCelPaginateButtons.style = celPaginateButtonsCss;\n\nexport { CelPaginateButtons as cel_paginate_buttons };\n"]}}