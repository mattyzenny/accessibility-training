 = 'account-menu-item';\n    this.footerLinkClass = 'footer-link';\n    this.externalLinkIconColor = CelIconColors.GRAYDARKER;\n    this.externalLinkIconHoverColor = CelIconColors.PRIMARY_SHADE;\n    this.makeSignoutButtonFocused = false;\n    this.isUserImageBackgroundCalculated = false;\n    this.userFirstName = '';\n    this.userLastName = '';\n    this.accountMenuItems = [];\n    this.avatarImageSrc = './assets/avatars/students/1x/18_student.png';\n    this.signOutButtonConfig = {\n      id: 'sign-out-id',\n      label: 'Sign out',\n    };\n    this.copyrightLabel = 'Copyright © &year Savvas Learning Company LLC. and its affiliates. All Rights Reserved.';\n    this.agreementLabel = 'User Agreement';\n    this.agreementHref = '';\n    this.privacyLabel = 'Privacy Policy';\n    this.privacyHref = '';\n    this.creditsLabel = 'Credits';\n    this.creditsHref = '';\n    this.revisionTag = '';\n    this.showRevisionTag = true;\n    this.showCredits = true;\n    this.hideSignOut = false;\n    this.hideAvatar = false;\n    this.parentTargetElement = null;\n    this.openInNewTabLinkText = 'This link will open in an external page and logout is still required';\n  }\n  connectedCallback() {\n    this.cel_button = transformTagName('cel-button', 'cel-account-menu-component', this.el);\n    this.cel_text_link = transformTagName('cel-text-link', 'cel-account-menu-component', this.el);\n    this.cel_icon = transformTagName('cel-icon', 'cel-account-menu-component', this.el);\n  }\n  /**\n   * listener to keydown event\n   */\n  handleKeyDown(event) {\n    const activeElement = this.el.shadowRoot.activeElement;\n    const activeElementClassList = activeElement.classList;\n    switch (event.key) {\n      case ENTER:\n      case SPACEBAR:\n      case SPACEBAR_CHAR:\n        if (activeElementClassList.contains(this.accountMenuItemClass)) {\n          this.accountMenuItems.forEach(item => {\n            if (item.label === activeElement.textContent) {\n              this.onAccountMenuItemClick(item);\n            }\n          });\n        }\n        break;\n      case ESCAPE:\n        this.onHandleCloseEvent();\n        break;\n      case ARROW_DOWN:\n      case ARROW_UP:\n        if (activeElementClassList.contains(this.accountMenuItemClass)\n          || activeElementClassList.contains(this.footerLinkClass)) {\n          this.handleArrowUpDownNavigation(event.key, activeElement);\n        }\n    }\n  }\n  handleArrowUpDownNavigation(keyPressed, activeElement) {\n    const parentElement = activeElement.parentElement;\n    if (keyPressed === ARROW_DOWN) {\n      this.onMakeMenuItemFocused(parentElement, 'nextSibling');\n    }\n    else if (keyPressed === ARROW_UP) {\n      this.onMakeMenuItemFocused(parentElement, 'previousSibling');\n    }\n  }\n  onMakeMenuItemFocused(currentActiveItem, siblingElement) {\n    var _a;\n    const toBeActiveElement = (_a = currentActiveItem[siblingElement]) === null || _a === void 0 ? void 0 : _a.firstChild;\n    if (toBeActiveElement) {\n      this.makeItemFocused(toBeActiveElement);\n    }\n    else {\n      if (siblingElement === 'previousSibling') {\n        const toBeActiveElementFirstSibling = currentActiveItem.parentElement.lastChild.firstChild;\n        this.makeItemFocused(toBeActiveElementFirstSibling);\n      }\n      else {\n        const toBeActiveElementLastSibling = currentActiveItem.parentElement.firstChild.firstChild;\n        this.makeItemFocused(toBeActiveElementLastSibling);\n      }\n    }\n  }\n  makeItemFocused(item) {\n    var _a;\n    if (item) {\n      let itemToBeFocusable = item;\n      if (item.classList.contains(this.footerLinkClass)) {\n        itemToBeFocusable = (_a = item === null || item === void 0 ? void 0 : item.shadowRoot) === null || _a === void 0 ? void 0 : _a.querySelector('div > a');\n      }\n      itemToBeFocusable.focus();\n    }\n  }\n  componentDidLoad() {\n    this.makeFirstItemFocused();\n  }\n  setImageSectionBackgroundColorFromUserImage() {\n    const userImageElement = this.el.shadowRoot.querySelector('.user-image');\n    const bgColor = getRGBColorFromImage(userImageElement);\n    const profile = this.el.shadowRoot.querySelector('.profile-image-container');\n    profile.style.background = `rgb(${bgColor.r}, ${bgColor.g}, ${bgColor.b}`;\n    this.isUserImageBackgroundCalculated = true;\n  }\n  makeFirstItemFocused() {\n    const focusableElements = this.accountMenuDivElement\n      .querySelectorAll(`.${this.accountMenuItemClass}`);\n    if (focusableElements === null || focusableElements === void 0 ? void 0 : focusableElements.length) {\n      const firstFocusableElement = focusableElements[0];\n      this.makeItemFocused(firstFocusableElement);\n    }\n    else {\n      this.makeSignoutButtonFocused = true;\n    }\n  }\n  onHandleCloseEvent() {\n    this.closeEvent.emit({ isClose: true, parentTargetElement: this.parentTargetElement });\n  }\n  onAccountMenuItemClick(item) {\n    this.accountMenuItemClick.emit(item);\n  }\n  loadRevisionTag() {\n    let revisionTag = null;\n    if (this.showRevisionTag) {\n      revisionTag = h(\"span\", { class: \"revision-tag\" }, this.revisionTag);\n      if (this.showCredits) {\n        revisionTag = h(\"div\", null, revisionTag);\n      }\n    }\n    return revisionTag;\n  }\n  render() {\n    const signOutLabel = this.signOutButtonConfig\n      && this.signOutButtonConfig.label ? this.signOutButtonConfig.label : '';\n    const currentYear = String(new Date().getFullYear());\n    const logoSrc = 'https://lst-assets.savvasrealize.com/assets/logos/savvas-gray.svg';\n    let formattedCopyrightLabel = '';\n    if (this.copyrightLabel) {\n      formattedCopyrightLabel = this.copyrightLabel.replace('&year', currentYear);\n    }\n    return (h(Host, { \"data-cel-version\": '9.0.2' }, h(\"div\", { class: \"account-menu-container\", ref: (el) => (this.accountMenuDivElement = el) }, this.hideAvatar ?\n      null :\n      h(\"div\", { class: \"profile-image-container account-menu-section\" }, h(\"img\", { class: `user-image ${this.isUserImageBackgroundCalculated ? '' : 'hidden'}`, src: this.avatarImageSrc, alt: navbarAvatarImgAltText, \"aria-hidden\": \"true\", onLoad: () => this.setImageSectionBackgroundColorFromUserImage() })), h(\"div\", { class: \"username-container account-menu-section\" }, h(\"div\", { class: \"first-name\" }, this.userFirstName), h(\"div\", { class: \"last-name\" }, this.userLastName)), this.accountMenuItems && this.accountMenuItems.length\n      ? (h(\"div\", { class: \"menu-items-container account-menu-section\" }, h(\"ul\", { role: \"menu\" }, this.accountMenuItems.map((item) => (h(\"li\", { role: \"presentation\" }, h(\"a\", { class: `${this.accountMenuItemClass}`, role: \"menuitem\", href: item.href, onClick: () => this.onAccountMenuItemClick(item), \"aria-label\": item.isExternalLink ? `${item.label} ${this.openInNewTabLinkText}` : item.label, tabindex: \"0\" }, h(\"span\", { class: item.isExternalLink ? 'label-width' : '' }, item.label), item.isExternalLink ? (h(this.cel_icon, { class: \"external-link-icon\", \"aria-hidden\": \"true\", \"data-name\": \"maximize\", \"data-color\": this.externalLinkIconColor })) : null))))))) : null, this.hideSignOut ?\n      null :\n      h(\"div\", { class: \"sign-out-action-container account-menu-section\" }, h(this.cel_button, { \"data-focus-element\": this.makeSignoutButtonFocused, class: \"sign-out-btn\", \"data-variant\": \"outline\", \"data-color\": \"primary\", \"data-label\": signOutLabel, onClick: () => this.onAccountMenuItemClick(this.signOutButtonConfig) })), h(\"div\", { class: \"footer-container account-menu-section\" }, h(\"div\", { class: \"copyright-info\" }, formattedCopyrightLabel), h(\"ul\", { class: \"links-info\", role: \"presentation\" }, h(\"li\", null, h(this.cel_text_link, { class: this.footerLinkClass, \"data-label\": this.agreementLabel, \"data-link-href\": this.agreementHref, \"data-open-in-new-tab\": \"true\", \"data-external-link-icon-color\": this.externalLinkIconColor, \"data-external-link-icon-hovered-color\": this.externalLinkIconHoverColor, \"data-text-link-aria-label\": `${this.agreementLabel} ${this.openInNewTabLinkText}` }), h(\"span\", { class: \"link-divider\" })), h(\"li\", null, h(this.cel_text_link, { class: this.footerLinkClass, \"data-label\": this.privacyLabel, \"data-link-href\": this.privacyHref, \"data-open-in-new-tab\": \"true\", \"data-external-link-icon-color\": this.externalLinkIconColor, \"data-external-link-icon-hovered-color\": this.externalLinkIconHoverColor, \"data-text-link-aria-label\": `${this.privacyLabel} ${this.openInNewTabLinkText}` }), this.showCredits ? h(\"span\", { class: \"link-divider\" }) : null), this.showCredits ?\n      h(\"li\", null, h(this.cel_text_link, { class: this.footerLinkClass, \"data-label\": this.creditsLabel, \"data-link-href\": this.creditsHref, \"data-open-in-new-tab\": \"true\", \"data-external-link-icon-color\": this.externalLinkIconColor, \"data-external-link-icon-hovered-color\": this.externalLinkIconHoverColor, \"data-text-link-aria-label\": `${this.creditsLabel} ${this.openInNewTabLinkText}` })) : null), this.loadRevisionTag(), h(\"div\", { class: \"logo-section\" }, h(\"img\", { class: !this.showRevisionTag ? 'increased-margin-top' : '', src: logoSrc, alt: \"Savvas logo\" }))))));\n  }\n  get el() { return getElement(this); }\n};\nCelAccountMenuComponent.style = celAccountMenuComponentCss;\n\nexport { CelAccountMenuComponent as cel_account_menu_component };\n"]}}