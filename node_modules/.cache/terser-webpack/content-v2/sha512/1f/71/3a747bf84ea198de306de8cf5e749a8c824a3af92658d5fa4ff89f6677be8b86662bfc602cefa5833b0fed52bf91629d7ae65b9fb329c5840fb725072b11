,\n        on,\n        onChange,\n        use,\n        dispose,\n        reset,\n        forceUpdate,\n    };\n};\nconst removeFromArray = (array, item) => {\n    const index = array.indexOf(item);\n    if (index >= 0) {\n        array[index] = array[array.length - 1];\n        array.length--;\n    }\n};\n\nconst createStore = (defaultState, shouldUpdate) => {\n    const map = createObservableMap(defaultState, shouldUpdate);\n    map.use(stencilSubscription());\n    return map;\n};\n\nconst Embed = Quill.import('blots/embed');\nclass MathEmbedBlot extends Embed {\n  static create(data) {\n    var _a, _b, _c, _d, _e, _f, _g, _h;\n    const { attributes = {} } = data;\n    let node = super.create();\n    // TODO: See if this can be refactored\n    // Math editor gets restored through Quill Undo Redo function\n    // The attribute comes as NamedNodeMap.\n    // To read the attribute names need to use value property\n    node.setAttribute('contenteditable', 'false');\n    if (data.ELEMENT_NODE) {\n      node.setAttribute('data-latex', ((_a = attributes['data-latex']) === null || _a === void 0 ? void 0 : _a.value) || '');\n      node.setAttribute('data-previous-latex', ((_b = attributes['data-previous-latex']) === null || _b === void 0 ? void 0 : _b.value) || '');\n      node.setAttribute('data-cel-icon-base-url', ((_c = attributes['data-cel-icon-base-url']) === null || _c === void 0 ? void 0 : _c.value) || '');\n      node.setAttribute('data-math-editor-mode', (_d = attributes['data-math-editor-mode']) === null || _d === void 0 ? void 0 : _d.value);\n      node.setAttribute('data-enable-math-editor', (_e = attributes['data-enable-math-editor']) === null || _e === void 0 ? void 0 : _e.value);\n      node.setAttribute('data-math-editor-focus-mode', (_f = attributes['data-math-editor-focus-mode']) === null || _f === void 0 ? void 0 : _f.value);\n      node.setAttribute('data-cel-button-tag-name', (_g = attributes['data-cel-button-tag-name']) === null || _g === void 0 ? v