yle:var(--text-area-border-style, solid);border-color:var(--text-area-border-color, #767676);border-radius:var(--rich-text-editor-border-radius, 0px 0px 4px 4px);color:var(--rich-text-editor-font-color, #333)}.ql-container.ql-snow .ql-editor.ql-blank::before{font-family:OpenSans;font-size:var(--cel-rte-placeholder-font-size, 1rem);line-height:24px;color:#767676}.sc-quill-cel-rich-text-editor-title{font-family:OpenSans-Semibold;font-size:0.75rem;line-height:16px;color:#333;margin-bottom:4px}\";\n\nconst CelRichText = class {\n  constructor(hostRef) {\n    registerInstance(this, hostRef);\n    this.textChanges = createEvent(this, \"celRichTextChanges\", 7);\n    this.countChange = createEvent(this, \"celRichTextCountChange\", 7);\n    this.textAltered = createEvent(this, \"celRichTextAltered\", 7);\n    this.mathPaletteActive = createEvent(this, \"celRichTextMathPaletteActive\", 7);\n    this.componentLoaded = createEvent(this, \"celRichTextLoaded\", 7);\n    this.celRichTextService = celRichTextInstance;\n    this.toolbarItemList = rteBasicToolbar;\n    this.setCursorPositionOnMathPaletteClose = true;\n    this.quillEditor = null;\n    this.isError = false;\n    this.mathContentScreenReaderInfo = '';\n    this.editorAriaLabel = 'Rich Text Edit Box';\n    this.identifier = undefined;\n    this.showComponentLoadInfo = false;\n    this.celIconBaseUrl = '';\n    this.editorTitle = '';\n    this.placeholderText = '';\n    this.rteAriaLabel = 'Entering rich text edit area';\n    this.toolbarAriaLabel = 'Text Formatting';\n    this.rteContents = '';\n    this.debounceInterval = 0;\n    this.maxCharLength = 0;\n    this.showFooterButton = false;\n    this.footerButtonDisabled = true;\n    this.rteButtonLabel = 'Done';\n    this.rteStatus = undefined;\n    this.rteMode = 'editor';\n    this.enableMathEditor = false;\n    this.reviewModeAriaLabel = constants.reviwModeAriaLabel;\n    this.closeActiveMathPalette = false;\n    this.mathKeypadTitle = 'Math Keypad';\n    this.stickKeypadToBottom = false;\n    this.dragIconAriaLabel = 'Drag icon. Use arrow keys to change the position of the keypad';\n    this.keypadLeftArrowDisabledLabel = 'Scroll Left Disabled';\n    this.keypadRightArrowDisabledLabel = 'Scroll Right Disabled';\n  }\n  handlerRteStatus() {\n    var _a;\n    if ((_a = this.editorStore$) === null || _a === void 0 ? void 0 : _a.state) {\n      this.editorStore$.state.rteStatus = this.rteStatus;\n    }\n  }\n  handleModeChange() {\n    if (this.rteMode === 'editor' && this.quillEditor === null) {\n      setTimeout(() => {\n        this.loadEditor();\n      }, 100);\n    }\n  }\n  handleCloseActiveMathPalette() {\n    var _a;\n    if (this.closeActiveMathPalette && ((_a = this.editorStore$) === null || _a === void 0 ? void 0 : _a.state)) {\n      this.setCursorPositionOnMathPaletteClose = false;\n      this.celRichTextService.closeActiveMathPalette(this.quillEditor, this.editorStore$);\n    }\n  }\n  // Method called from parent component to get content of the editor.\n  async getContents() {\n    if (this.quillEditor) {\n      const content = this.celRichTextService.getContents(this.quillEditor);\n      return this.transformedContents(this.cel_rich_text_math_editor, constants.defaultMathEditorTagName, content);\n    }\n    else {\n      return '';\n    }\n  }\n  // Method called from parent component to clear content of the editor.\n  async clearContents() {\n    if (this.quillEditor) {\n      this.celRichTextService.clearContents(this.quillEditor);\n    }\n  }\n  // Method sets focus to editor & sets position of cursor\n  async focusTextEditor() {\n    setTimeout(() => {\n      if (this.quillEditor) {\n        this.celRichTextService.focusEditor(this.quillEditor, this.editorStore$);\n      }\n    });\n  }\n  // when a user clicks on the address bar while the math palette window is already open state,\n  // and if user doing shift+tab and trying to open the math palette on another RTE, close existing math palette window\n  handleClose(ev) {\n    var _a;\n    ev.stopPropagation();\n    if (this.enableMathEditor) {\n      const targetDataSet = (ev === null || ev === void 0 ? void 0 : ev.target) ? ev === null || ev === void 0 ? void 0 : ev.target[constants.dataset] : '';\n      const { name = '' } = targetDataSet;\n      if (ev.key === ENTER && name === toolbarIconName.mathembed) {\n        if (((_a = this.editorStore$) === null || _a === void 0 ? void 0 : _a.state) && this.editorStore$.state.activeMathEditorStatus && !targetDataSet.hasOwnProperty(constants.disabled)) {\n          this.setCursorPositionOnMathPaletteClose = false;\n          this.celRichTextService.closeActiveMathPalette(this.quillEditor, this.editorStore$);\n        }\n      }\n    }\n  }\n  handleCelRichTextChanges(eventObj) {\n    this.rteStatus = { message: '', status: 'saving' };\n    const { detail: { action, content } } = eventObj;\n    let saveContent = content;\n    if (content) {\n      const _content = this.celRichTextService.getContents(this.quillEditor);\n      saveContent = this.transformedContents(this.cel_rich_text_math_editor, constants.defaultMathEditorTagName, _content);\n    }\n    this.textChanges.emit({ id: this.identifier, action, content: saveContent });\n  }\n  handleCelRichTextFooterLoaded() {\n    this.handleFooterInitialValues();\n  }\n  handleCelRichTextMathEditorLoaded() {\n    this.quillEditor.root.setAttribute(\"contenteditable\", \"false\");\n  }\n  handleCelRichTextActiveMathEditorStatus(eventObj) {\n    const { detail: editorStatus } = eventObj;\n    this.editorStore$.state.activeMathEditorStatus = editorStatus;\n  }\n  handleMathEmbedFocusChange(eventObj) {\n    const { detail: mathString } = eventObj;\n    this.mathContentScreenReaderInfo = mathString;\n  }\n  handleErrorMessage(event) {\n    this.updatedAriaLabel = this.celRichTextService.getAriaLabel(this.quillEditor, this.editorAriaLabel, this.maxCharLength, event.detail.message, this.placeholderText);\n  }\n  connectedCallback() {\n    this.cel_rich_text_toolbar = transformTagName('cel-rich-text-toolbar', 'cel-rich-text', this.el);\n    this.cel_rich_text_footer = transformTagName('cel-rich-text-footer', 'cel-rich-text', this.el);\n    this.cel_rich_text_math_editor = transformTagName('cel-rich-text-math-editor', 'cel-rich-text', this.el);\n    this.cel_rich_text_math_expression_viewer = transformTagName('cel-rich-text-math-expression-viewer', 'cel-rich-text', this.el);\n    this.cel_rich_text_math_expression_mathquill_viewer = transformTagName('cel-rich-text-math-expression-mathquill-viewer', 'cel-rich-text', this.el);\n    this.cel_button = transformTagName('cel-button', 'cel-rich-text', this.el);\n  }\n  async componentWillLoad() {\n    const { textFormat, list } = this.extractToolOptions();\n    this.rteFixedValues = {\n      placeholderText: this.placeholderText,\n      editorAriaLabel: this.editorAriaLabel,\n      iconBaseUrl: this.celIconBaseUrl,\n      textFormats: textFormat,\n      listFormats: list,\n      mathKeypadTitle: this.mathKeypadTitle,\n      stickKeypadToBottom: this.stickKeypadToBottom,\n      dragIconAriaLabel: this.dragIconAriaLabel,\n      keypadRightArrowDisabledLabel: this.keypadRightArrowDisabledLabel,\n      keypadLeftArrowDisabledLabel: this.keypadLeftArrowDisabledLabel,\n    };\n    this.editorStore$ = await this.celRichTextService.initEditorStore();\n    this.toolbarStore$ = await this.celRichTextService.initToolbarStore();\n  }\n  // Called only once after render();\n  componentDidLoad() {\n    this.transformedRteContents = this.transformedContents(constants.defaultMathEditorTagName, this.cel_rich_text_math_editor, this.rteContents);\n    if (this.rteMode === 'editor') {\n      this.loadEditor();\n      this.editorStore$.onChange('textChange', ({ currentCharCount, currentRteText }) => {\n        this.onCountChange(currentCharCount);\n        this.onTextChange(currentRteText);\n        this.handleErrorState();\n      });\n      this.editorStore$.onChange('status', () => {\n        this.handleErrorState();\n      });\n      this.editorStore$.onChange('handleToolbar', (eventObj) => {\n        const { detail: { id: toolType } } = eventObj;\n        this.celRichTextService.handleToolbarAction(this.quillEditor, this.editorStore$, this.toolbarStore$, this.enableMathEditor, toolType, this.rteFixedValues, this.cel_button);\n      });\n      this.editorStore$.onChange('activeMathEditorStatus', (editorStatus) => {\n        if (editorStatus)\n          this.closeActiveMathPalette = false;\n        if (!editorStatus) {\n          this.celRichTextService.onMathPaletteClose(this.quillEditor, this.editorStore$, this.setCursorPositionOnMathPaletteClose);\n        }\n        this.setCursorPositionOnMathPaletteClose = true;\n        this.mathPaletteActive.emit(editorStatus);\n        const currentCharCount = this.celRichTextService.currentCharCount(this.quillEditor);\n        const currentRteText = this.celRichTextService.getContents(this.quillEditor);\n        this.editorStore$.state.textChange = { currentCharCount, currentRteText, source: 'user' };\n        this.toolbarStore$.state.enableToolbarIcon = !editorStatus;\n        this.celRichTextService.toggleUndoRedoStates(this.quillEditor, this.toolbarStore$);\n      });\n      this.quillEditor.root.setAttribute('aria-label', this.updatedAriaLabel);\n    }\n    else {\n      this.rteComponentLoaded();\n    }\n  }\n  loadEditor() {\n    this.initializeQuill();\n    this.rteComponentLoaded();\n    this.updatedAriaLabel = this.celRichTextService.getAriaLabel(this.quillEditor, this.editorAriaLabel, this.maxCharLength, '', this.placeholderText);\n    this.quillEditor.root.addEventListener('focus', () => {\n      this.quillEditor.root.setAttribute('aria-label', this.updatedAriaLabel);\n    });\n  }\n  reviewContent() {\n    let reviewContents = this.rteContents;\n    if (this.rteMode === 'viewer' && this.quillEditor) {\n      reviewContents = this.celRichTextService.getContents(this.quillEditor);\n      this.quillEditor = null;\n    }\n    const isMathTagsPresent = this.celRichTextService.isMathTagsPresent(constants.defaultMathEditorTagName, reviewContents);\n    if (isMathTagsPresent) {\n      const reviewContentsWithViewStyle = this.celRichTextService.updateStyleForViewMathExpression(constants.defaultMathEditorTagName, reviewContents);\n      const reviewContentMathEditorTags = this.celRichTextService.transformedMathEditorTag(constants.defaultMathEditorTagName, constants.span, reviewContentsWithViewStyle);\n      reviewContents = this.celRichTextService.transformedMathEditorTag(constants.defaultMathExpressionViewerTagName, constants.span, reviewContentMathEditorTags);\n    }\n    return reviewContents;\n  }\n  initializeQuill() {\n    const rteContainerElement = this.componentEl.querySelector('.sc-quill-cel-rich-text');\n    this.quillEditor = this.celRichTextService.initializeQuill(rteContainerElement, this.transformedRteContents, this.editorStore$, this.toolbarStore$, this.rteFixedValues, this.cel_rich_text_math_editor, this.cel_button, this.enableMathEditor, this.mathKeypadTitle, this.stickKeypadToBottom, this.dragIconAriaLabel, this.keypadRightArrowDisabledLabel, this.keypadLeftArrowDisabledLabel);\n    const currentCharCount = this.celRichTextService.currentCharCount(this.quillEditor);\n    this.handleFooterInitialValues();\n    this.onCountChange(currentCharCount);\n  }\n  rteComponentLoaded() {\n    var _a, _b, _c, _d;\n    if (this.showComponentLoadInfo) {\n      this.componentLoaded.emit(true);\n      this.updatedAriaLabel = this.celRichTextService.getAriaLabel(this.quillEditor, this.editorAriaLabel, this.maxCharLength, '', this.placeholderText);\n      (_b = (_a = this.quillEditor) === null || _a === void 0 ? void 0 : _a.root) === null || _b === void 0 ? void 0 : _b.setAttribute('aria-label', this.updatedAriaLabel);\n      (_d = (_c = this.quillEditor) === null || _c === void 0 ? void 0 : _c.root) === null || _d === void 0 ? void 0 : _d.addEventListener('focus', () => {\n        this.quillEditor.root.setAttribute('aria-label', this.updatedAriaLabel);\n      });\n    }\n  }\n  handleFooterInitialValues() {\n    if (this.quillEditor) {\n      const currentCharCount = this.celRichTextService.currentCharCount(this.quillEditor);\n      const currentRteText = this.celRichTextService.getContents(this.quillEditor);\n      this.editorStore$.state.handleFooter = { charCount: currentCharCount, rteText: currentRteText };\n    }\n  }\n  onCountChange(currentCharCount) {\n    this.countChange.emit({ id: this.identifier, count: currentCharCount });\n  }\n  onTextChange(currentRteText) {\n    const isTextChanged = (this.transformedRteContents !== currentRteText);\n    this.textAltered.emit(isTextChanged);\n  }\n  handleErrorState() {\n    const { state: { status } } = this.editorStore$;\n    const currentCharCount = this.celRichTextService.currentCharCount(this.quillEditor);\n    const isTextPersistApplicable = !((this.maxCharLength > 0) && (currentCharCount > this.maxCharLength));\n    this.isError = ((status === StatusTypes.error) || ((this.maxCharLength > 0) && !isTextPersistApplicable));\n  }\n  transformedContents(transformFrom, transformTo, content) {\n    let transformedContent;\n    const { defaultMathEditorTagName, defaultMathExpressionViewerTagName, defaultMathExpressionMathQuillViewerTagName, } = constants;\n    // Identify if the Math Tags presents, it maybe by default/namespaced tag\n    const isMathTagsPresent = this.celRichTextService.isMathTagsPresent(defaultMathEditorTagName, content);\n    // Transform the Math tag's to default tag, if RTE rendering only as namespaced component\n    // Basically if persisting the RTE content as namespaced component then transform to default Math tag\n    // if rendering back the persisted Math content into RTE as namespaced component, then transform from default Math tag to name spaced tag\n    if ((isMathTagsPresent && content && this.cel_rich_text_math_editor !== defaultMathEditorTagName)) {\n      // Transform the parent Math tag of Math Editor tag\n      const transformedMathEditorTags = this.celRichTextService.transformedMathEditorTag(transformFrom, transformTo, content);\n      // Identify the Math Expression Viewer tag, transform from either namespaced tag / default tag\n      const mathExpressionViewerTagFrom = (transformFrom !== defaultMathEditorTagName) ? this.cel_rich_text_math_expression_viewer : defaultMathExpressionViewerTagName;\n      // Identify the Math Expression Viewer tag, transform to either namespaced tag / default tag\n      const mathExpressionViewerTagTo = (transformTo !== defaultMathEditorTagName) ? this.cel_rich_text_math_expression_viewer : defaultMathExpressionViewerTagName;\n      // Transform the Math Expression Viewer tag to namespaced tag / default tag by input params\n      // Also render only correct equation in math expression viewer\n      const transformedMathExpressionViewTags = this.celRichTextService.transformedMathEditorTag(mathExpressionViewerTagFrom, mathExpressionViewerTagTo, transformedMathEditorTags);\n      // Identify the Math Quill Viewer tag present into Math Expression viewer\n      // Also render only incorrect equation in Math Expression viewer\n      const isMathErrorTagsPresent = this.celRichTextService.isMathTagsPresent(defaultMathExpressionMathQuillViewerTagName, content);\n      if (isMathErrorTagsPresent) {\n        const mathExpressionMathQuillViewerTagFrom = (transformFrom !== defaultMathEditorTagName) ? this.cel_rich_text_math_expression_mathquill_viewer : defaultMathExpressionMathQuillViewerTagName;\n        const mathExpressionMathQuillViewerTagTo = (transformTo !== defaultMathEditorTagName) ? this.cel_rich_text_math_expression_mathquill_viewer : defaultMathExpressionMathQuillViewerTagName;\n        const transformedMathExpressionMathQuillViewTags = this.celRichTextService.transformedMathEditorTag(mathExpressionMathQuillViewerTagFrom, mathExpressionMathQuillViewerTagTo, transformedMathExpressionViewTags);\n        transformedContent = transformedMathExpressionMathQuillViewTags;\n      }\n      else {\n        transformedContent = transformedMathExpressionViewTags;\n      }\n    }\n    else {\n      transformedContent = content;\n    }\n    return transformedContent;\n  }\n  extractToolOptions() {\n    const { textFormat } = this.toolbarItemList.find((tools) => {\n      if (tools.textFormat) {\n        return tools;\n      }\n    });\n    const { list } = this.toolbarItemList.find((tools) => {\n      if (tools.list) {\n        return tools;\n      }\n    });\n    return { textFormat, list };\n  }\n  render() {\n    return (h(Host, { \"data-cel-version\": '9.0.2' }, this.rteMode === 'viewer' ? (h(\"div\", { class: \"sc-quill-cel-rich-text-editor-container\" }, h(\"div\", { class: \"sc-quill-cel-rich-text ql-container ql-snow\" }, h(\"div\", { class: \"ql-editor\", tabindex: \"0\", \"aria-label\": this.reviewModeAriaLabel, \"aria-readonly\": \"true\", role: \"textbox\", innerHTML: this.reviewContent() })))) :\n      h(\"div\", { class: \"sc-quill-cel-rich-text-editor-container\" }, h(\"div\", { role: \"application\", \"aria-label\": this.rteAriaLabel }, this.editorTitle ? h(\"span\", { class: \"sc-quill-cel-rich-text-editor-title\" }, \" \", this.editorTitle) : null, h(\"div\", null, h(this.cel_rich_text_toolbar, { celRteEditorStore: this.editorStore$, celRteToolbarStore: this.toolbarStore$, \"data-cel-icon-base-url\": this.celIconBaseUrl, \"data-enable-math-editor\": this.enableMathEditor, \"data-toolbar-aria-label\": this.toolbarAriaLabel })), h(\"div\", { ref: (el) => (this.componentEl = el) }, h(\"div\", { class: \"cel-visually-hidden\", \"aria-live\": \"polite\" }, this.mathContentScreenReaderInfo), h(\"div\", { class: ['sc-quill-cel-rich-text', this.isError ? 'error' : ''\n        ].join(' ') }))), h(\"div\", null, h(this.cel_rich_text_footer, { celRteEditorStore: this.editorStore$, \"data-cel-icon-base-url\": this.celIconBaseUrl, \"data-rte-contents\": this.transformedRteContents, \"data-debounce-interval\": this.debounceInterval, \"data-rte-button-label\": this.rteButtonLabel, \"data-max-char-length\": this.maxCharLength, footerButtonDisabled: this.footerButtonDisabled, \"data-show-footer-button\": this.showFooterButton, \"data-rte-mode\": this.rteMode })))));\n  }\n  get el() { return getElement(this); }\n  static get watchers() { return {\n    \"rteStatus\": [\"handlerRteStatus\"],\n    \"rteMode\": [\"handleModeChange\"],\n    \"closeActiveMathPalette\": [\"handleCloseActiveMathPalette\"]\n  }; }\n};\nCelRichText.style = celRichTextCss;\n\nexport { CelRichText as cel_rich_text };\n","var commonjsGlobal = typeof globalThis !== 'undefined' ? globalThis : typeof window !== 'undefined' ? window : typeof global !== 'undefined' ? global : typeof self !== 'undefined' ? self : {};\n\nfunction getDefaultExportFromCjs (x) {\n\treturn x && x.__esModule && Object.prototype.hasOwnProperty.call(x, 'default') ? x['default'] : x;\n}\n\nfunction createCommonjsModule(fn, basedir, module) {\n\treturn module = {\n\t\tpath: basedir,\n\t\texports: {},\n\t\trequire: function (path, base) {\n\t\t\treturn commonjsRequire();\n\t\t}\n\t}, fn(module, module.exports), module.exports;\n}\n\nfunction commonjsRequire () {\n\tthrow new Error('Dynamic requires are not currently supported by @rollup/plugin-commonjs');\n}\n\nexport { commonjsGlobal as a, createCommonjsModule as c, getDefaultExportFromCjs as g };\n"]}}