                }\n                return true;\n              }\n            },\n            enter: {\n              key: 13,\n              handler: function (range) {\n                const currentContent = quillEditor.getContents(range.index, 1);\n                if (currentContent.ops[0].attributes && currentContent.ops[0].attributes.type === 'math' && enableMathEditor) {\n                  currentContent.ops[0].attributes.node.setAttribute('data-math-editor-mode', 'editor');\n                }\n                else {\n                  const { state: { activeMathEditorStatus } } = celRichTextStore;\n                  if (!activeMathEditorStatus) {\n                    return true;\n                  }\n                }\n              }\n            },\n            'indent': {\n              // highlight tab or tab at beginning of list, indent or blockquote\n              key: 9,\n              format: ['list'],\n              handler: () => true,\n            },\n          }\n        },\n      },\n      theme: 'snow',\n      placeholder: rteFixedValues.placeholderText,\n    });\n    quillEditor.root.setAttribute('role', 'textbox');\n    // quillEditor.root.setAttribute('aria-label', rteFixedValues.editorAriaLabel);\n    this.bindQuillEvents(quillEditor, rteContents, celRichTextStore, celRichTextToolbarStore, enableMathEditor, rteFixedValues, celRichTextMathEditorNodeName, celButtonTagName, mathKeypadTitle, stickKeypadToBottom, dragIconAriaLabel, keypadRightArrowDisabledLabel, keypadLeftArrowDisabledLabel);\n    return quillEditor;\n  }\n  // Initialize RTE store\n  async initEditorStore() {\n    const { state, onChange } = await createStore({\n      textChange: null,\n      status: null,\n      selectedRange: { index: 0, length: 0 },\n      handleToolbar: null,\n      handleFooter: null,\n      rteStatus: null,\n      activeMathEditorStatus: false,\n      mathEmbedIndex: -1,\n    });\n    return { state, onChange };\n  }\n  // Initialize RTE toolbar store to maintain formats and enabling / disabling of toolbar options\n  async initToolbarStore() {\n    const { state, onChange } = await createStore({\n      bold: false,\n      italic: false,\n      underline: false,\n      bullet: false,\n      ordered: false,\n      undo: false,\n      redo: false,\n      enableToolbarIcon: true,\n    });\n    return { state, onChange };\n  }\n  // Called when Palette is closed either by clicking the cross icon or the save button\n  onMathPaletteClose(quillEditor, celRichTextStore, rteFocus = true) {\n    const currentSelection = this.currentSelection(quillEditor);\n    const { index = 0, length = 0 } = currentSelection ? currentSelection : celRichTextStore.state.selectedRange;\n    const { state: { mathEmbedIndex } } = celRichTextStore;\n    const embedIndex = mathEmbedIndex >= 0 ? mathEmbedIndex : index;\n    const currentNode = this.getNodeData(embedIndex, quillEditor);\n    let currentIndex;\n    if (currentNode.type === 'math') {\n      if (currentNode.node.getAttribute('data-latex')) {\n        if (quillEditor.getText(index + 1, 1) !== SPACEBAR_CHAR) {\n          quillEditor.insertText(index + 1, SPACEBAR_CHAR, 'user');\n        }\n        currentIndex = { index: index + 2, length };\n      }\n      else {\n        // delete the math embed with space which add on insert/edit embed\n        quillEditor.deleteText(index, 2, 'user');\n        currentIndex = { index, length };\n      }\n    }\n    else {\n      /* Math equation on click - selection-change index will give the next index.\n       To handle that scenario, taking the previous index and checking for math node */\n      const previousIndex = index - 1;\n      const previousNode = this.getNodeData(previousIndex, quillEditor);\n      if (previousIndex >= 0 && (previousNode.type === 'math') && (previousNode.node.getAttribute('data-latex'))) {\n        if (quillEditor.getText(previousIndex + 1, 1) !== SPACEBAR_CHAR) {\n          quillEditor.insertText(previousIndex + 1, SPACEBAR_CHAR, 'user');\n        }\n        currentIndex = { index: previousIndex + 2, length };\n      }\n      else {\n        quillEditor.deleteText(previousIndex, 2, 'user');\n        currentIndex = { index: previousIndex, length };\n      }\n    }\n    celRichTextStore.state.selectedRange = currentIndex;\n    celRichTextStore.state.mathEmbedIndex = -1;\n    if (!rteFocus) {\n      quillEditor.root.setAttribute(\"contenteditable\", \"true\");\n    }\n    else {\n      setTimeout(() => {\n        quillEditor.root.setAttribute(\"contenteditable\", \"true\");\n        this.setCursorPosition(quillEditor, celRichTextStore, 'user');\n      });\n    }\n  }\n  // When math equation is rendered in view mode, add a css class to it\n  updateStyleForViewMathExpression(mathEditorTagName, rteContent) {\n    let styleUpdatedContent = rteContent;\n    const mathEditorTagArray = this.getMathEditorTags(mathEditorTagName, rteContent);\n    if (mathEditorTagArray && mathEditorTagArray.length > 0) {\n      const mathEditorTag = mathEditorTagArray[0];\n      const regExp = new RegExp(mathEditorTag, 'g');\n      styleUpdatedContent = rteContent.replace(regExp, `${mathEditorTag} class='${constants.mathExpressionEmbedViewerClassName}'`);\n    }\n    return styleUpdatedContent;\n  }\n  isMathTagsPresent(mathEditorTagName, rteContent) {\n    const mathEditorTagArray = this.getMathEditorTags(mathEditorTagName, rteContent);\n    return (mathEditorTagArray && mathEditorTagArray.length > 0);\n  }\n  // Replace the incoming RTE contents math editor node with the current node name of math editor (namespace scenario)\n  transformedMathEditorTag(transformFrom, transformTo, rteContent) {\n    let transformedContent = rteContent;\n    if (rteContent) {\n      const mathEditorTagName = this.getMathEditorTagName(transformFrom, rteContent);\n      const regExp = new RegExp(mathEditorTagName, 'g');\n      transformedContent = rteContent.replace(regExp, transformTo);\n    }\n    return transformedContent;\n  }\n  // Get the tag name of math editor saved in RTE. This check will ensure if there were any\n  // namespace made earlier for math editor, it will get converted to the current node name used.\n  getMathEditorTagName(transformTag, rteContent) {\n    let mathEditorTagName = transformTag;\n    const mathEditorTagArray = this.getMathEditorTags(transformTag, rteContent);\n    if (mathEditorTagArray && mathEditorTagArray.length > 0) {\n      const regExp = new RegExp('<', 'g');\n      const mathEditorTag = mathEditorTagArray[0];\n      mathEditorTagName = mathEditorTag.replace(regExp, '').trim();\n    }\n    return mathEditorTagName;\n  }\n  getMathEditorTags(mathEditorTagName, rteContent) {\n    const regularExpression = new RegExp('<([a-zA-Z-]*)' + mathEditorTagName + SPACEBAR_CHAR);\n    return rteContent.match(regularExpression);\n  }\n  // Returns the node name and type of a character at location in RTE\n  getNodeData(index, quillEditor) {\n    var _a;\n    const nodeContent = quillEditor.getContents(index, 1);\n    const nodeContentAttrObj = ((_a = nodeContent === null || nodeContent === void 0 ? void 0 : nodeContent.ops[0]) === null || _a === void 0 ? void 0 : _a.attributes) || {};\n    const { type = null, node = null } = nodeContentAttrObj;\n    return {\n      type,\n      node\n    };\n  }\n  // When user clicks on RTE, based on its content length place the cursor at the beginning or end\n  setCursorPosition(quillEditor, celRichTextStore, source) {\n    try {\n      const contentLength = this.currentCharLength(quillEditor);\n      if (contentLength > 0) {\n        const selectedRange = celRichTextStore.state.selectedRange;\n        const { index, length } = selectedRange ? selectedRange : { index: 0, length: 0 };\n        if (selectedRange && index !== 0) {\n          quillEditor.setSelection(index, length, source);\n        }\n        else {\n          quillEditor.setSelection(contentLength, contentLength, source);\n        }\n      }\n      else {\n        quillEditor.setSelection(0, 0, source);\n      }\n    }\n    catch (e) {\n      console.error('RTE cursor position handling exception', e);\n    }\n  }\n  clickOnFormatTool(quillEditor, celRichTextStore, celRichTextToolbarStore, toolType, textFormats, action) {\n    const { state: { selectedRange } } = celRichTextStore;\n    if (!selectedRange || selectedRange.length === 0) {\n      textFormats.forEach((format) => {\n        if (toolType === format) {\n          const state = !celRichTextToolbarStore.state[format];\n          celRichTextToolbarStore.state[format] = state;\n        }\n      });\n      quillEditor.format(toolType, celRichTextToolbarStore.state[toolType], 'user');\n    }\n    else {\n      const { index, length } = selectedRange;\n      const _currentFormat = quillEditor.getFormat(index, length);\n      const state = (action === 'click') ? !_currentFormat[toolType] : _currentFormat[toolType];\n      celRichTextToolbarStore.state[toolType] = state;\n      // Format the text user selected from tool and for shortcut's quill handling internally\n      if (action === 'click') {\n        quillEditor.formatText(index, length, toolType, celRichTextToolbarStore.state[toolType], 'user');\n      }\n    }\n  }\n  clickOnListTool(quillEditor, celRichTextStore, celRichTextToolbarStore, listFormats, toolType) {\n    const { state: { selectedRange } } = celRichTextStore;\n    listFormats.forEach((format) => {\n      if (format == toolType) {\n        const state = !celRichTextToolbarStore.state[toolType];\n        celRichTextToolbarStore.state[format] = state;\n      }\n      else {\n        celRichTextToolbarStore.state[format] = false;\n      }\n    });\n    if (!selectedRange || selectedRange.length === 0) {\n      quillEditor.format('list', celRichTextToolbarStore.state[toolType] ? toolType : false, 'user');\n    }\n    else {\n      const { index, length } = selectedRange;\n      quillEditor.formatLine(index, length, 'list', celRichTextToolbarStore.state[toolType] ? toolType : false, 'user');\n    }\n  }\n  insertMathEmbed(quillEditor, enableMathEditor, latex, index, iconBaseUrl, celButtonTagName, celRichTextStore, mathKeypadTitle, stickKeypadToBottom, dragIconAriaLabel, keypadRightArrowDisabledLabel, keypadLeftArrowDisabledLabel) {\n    //Add math editor here\n    const insertAttributes = {\n      attributes: {\n        'data-latex': latex,\n        'data-previous-latex': latex,\n        'data-math-editor-mode': 'editor',\n        'data-enable-math-editor': enableMathEditor,\n        'data-math-editor-focus-mode': '',\n        'data-cel-icon-base-url': iconBaseUrl,\n        'data-cel-button-tag-name': celButtonTagName,\n        'data-math-keypad-title': mathKeypadTitle,\n        'data-stick-keypad-to-bottom': stickKeypadToBottom,\n        'data-drag-icon-aria-label': dragIconAriaLabel,\n        'data-keypad-right-arrow-disabled-label': keypadRightArrowDisabledLabel,\n        'data-keypad-left-arrow-disabled-label': keypadLeftArrowDisabledLabel,\n      },\n    };\n    const previousIndex = ((index - 1) >= 0) ? (index - 1) : 0;\n    const isPreviousIndexMathEmbed = this.isMathEmbedExistByIndex(previousIndex, quillEditor);\n    let mathEmbedRange = { index: 0, length: 0 };\n    let embedIndex = index;\n    let nextIndex = index + 1;\n    mathEmbedRange = { index: embedIndex, length: nextIndex };\n    if (isPreviousIndexMathEmbed) {\n      // Adding extra space for avoiding the Maximum call stack size exceeded exception \n      quillEditor.insertText(index, SPACEBAR_CHAR, 'silent');\n      embedIndex = index + 1;\n      nextIndex = index + 2;\n    }\n    celRichTextStore.state.mathEmbedIndex = embedIndex;\n    quillEditor.insertEmbed(embedIndex, 'mathEmbed', insertAttributes, 'silent');\n    const nextIndexText = this.getTextByIndex(nextIndex, quillEditor);\n    if (nextIndexText !== SPACEBAR_CHAR) {\n      quillEditor.insertText(nextIndex, SPACEBAR_CHAR, 'silent');\n      mathEmbedRange = { index, length: nextIndex };\n    }\n    else {\n      mathEmbedRange = { index, length: index + 1 };\n    }\n    celRichTextStore.state.selectedRange = mathEmbedRange;\n  }\n  bindQuillEvents(quillEditor, rteContents, celRichTextStore, celRichTextToolbarStore, enableMathEditor, rteFixedValues, celRichTextMathEditorNodeName, celButtonTagName, mathKeypadTitle, stickKeypadToBottom, dragIconAriaLabel, keypadRightArrowDisabledLabel, keypadLeftArrowDisabledLabel) {\n    const { textFormats } = rteFixedValues;\n    quillEditor.clipboard.addMatcher('div', (node) => {\n      if (node.classList.contains('cel-rich-text-math-container') ||\n        node.classList.contains('cel-rich-text-math-container-holder') ||\n        node.classList.contains('cel-rich-text-view-container') ||\n        node.classList.contains('cel-visually-hidden')) {\n        return new Delta().insert('');\n      }\n      return new Delta().insert(node.textContent);\n    });\n    quillEditor.clipboard.addMatcher(celRichTextMathEditorNodeName, (node) => {\n      node.innerHTML = '';\n      const { iconBaseUrl } = rteFixedValues;\n      let insertMathEmbedWithSpace = new Delta();\n      insertMathEmbedWithSpace = {\n        ops: [\n          {\n            insert: {\n              mathEmbed: {\n                attributes: {\n                  'data-latex': node.getAttribute('data-latex'),\n                  'data-previous-latex': node.getAttribute('data-latex'),\n                  'data-math-editor-mode': 'load',\n                  'data-enable-math-editor': enableMathEditor,\n                  'data-math-editor-focus-mode': '',\n                  'data-cel-icon-base-url': iconBaseUrl,\n                  'data-cel-button-tag-name': celButtonTagName,\n                  'data-math-keypad-title': mathKeypadTitle,\n                  'data-stick-keypad-to-bottom': stickKeypadToBottom,\n                  'data-drag-icon-aria-label': dragIconAriaLabel,\n                  'data-keypad-right-arrow-disabled-label': keypadRightArrowDisabledLabel,\n                  'data-keypad-left-arrow-disabled-label': keypadLeftArrowDisabledLabel,\n                }\n              }\n            }\n          },\n          { insert: SPACEBAR_CHAR }\n        ]\n      };\n      return insertMathEmbedWithSpace;\n    });\n    // Ensure safe HTML init to editor.\n    const deltaToSet = quillEditor.clipboard.convert(rteContents);\n    quillEditor.setContents(deltaToSet, 'silent');\n    // Clearing history space add in add matcher if existing content rendered with math data\n    quillEditor.history.clear();\n    // Prevents special paste by, just loads text into editor when doing copy paste within the editor.\n    quillEditor.clipboard.addMatcher(Node.ELEMENT_NODE, (node, _delta) => {\n      return new Delta().insert(node.textContent);\n    });\n    // Handle character limit and character count\n    quillEditor.on('text-change', (_delta, _old, _source) => {\n      if (_source === 'user')\n        this.textChange(false, quillEditor, celRichTextStore, celRichTextToolbarStore, rteFixedValues, _source);\n    });\n    quillEditor.on('selection-change', (_range, _oldRange, _source) => {\n      const { state: { selectedRange: previousSelectedRange, activeMathEditorStatus } } = celRichTextStore;\n      if (_range && _source === 'user' && !activeMathEditorStatus) {\n        const isRetainableFocusedIndexFormat = this.isRetainableFocusedIndexFormat(previousSelectedRange, _range, quillEditor);\n        celRichTextStore.state.selectedRange = _range;\n        if ((_range.length) == 0 && (_oldRange == null))\n          this.setCursorPosition(quillEditor, celRichTextStore, 'user');\n        if (isRetainableFocusedIndexFormat) {\n          // To retain selected formats when user being in last index and changed format using toolbar\n          // then click outside and click inside in the same last index\n          this.retainEditorFormat(quillEditor, celRichTextToolbarStore, rteFixedValues);\n        }\n        else {\n          // To retain formats as user selected index in RTE text\n          this.handleRetainFormat(quillEditor, rteFixedValues, celRichTextStore, celRichTextToolbarStore, _range);\n        }\n        // Handle focus in and focus out for math embed\n        if (_oldRange) {\n          const { index: previousIndex } = _oldRange;\n          const previousRange = { index: previousIndex, length: 1 };\n          const previousFormat = quillEditor.getFormat(previousRange);\n          const previousFormatMode = (previousFormat && previousFormat.node) ? previousFormat.node.getAttribute('data-math-editor-mode') : null;\n          if (previousFormat.type === 'math' && (previousFormatMode === 'viewer' || previousFormatMode === 'load') && enableMathEditor) {\n            previousFormat.node.setAttribute('data-math-editor-focus-mode', 'focusout');\n          }\n        }\n        const { index: currentIndex } = _range;\n        const currentRage = { index: currentIndex, length: 1 };\n        const currentFormat = quillEditor.getFormat(currentRage);\n        const currentFormatMode = (currentFormat && currentFormat.node) ? currentFormat.node.getAttribute('data-math-editor-mode') : null;\n        if (currentFormat.type === 'math' && (currentFormatMode === 'viewer' || currentFormatMode === 'load') && enableMathEditor) {\n          currentFormat.node.setAttribute('data-math-editor-focus-mode', 'focusin');\n        }\n      }\n    });\n    quillEditor.root.addEventListener('blur', () => {\n      const highlightedMathNodes = quillEditor.root.getElementsByClassName('highlight-editor');\n      Array.from(highlightedMathNodes).forEach(function (element) {\n        element.setAttribute('data-math-editor-focus-mode', 'focusout');\n      });\n    });\n    quillEditor.root.addEventListener('keydown', (event) => {\n      const { ctrlKey, keyCode: ctrlKeyCode, metaKey } = event;\n      if (!ctrlKey && !metaKey)\n        return;\n      const { BOLD, ITALIC, UNDERLINE, UNDO, REDO } = KEYCODE;\n      switch (ctrlKeyCode) {\n        case BOLD: // Ctrl + B \n          this.clickOnFormatTool(quillEditor, celRichTextStore, celRichTextToolbarStore, 'bold', textFormats, 'shortcut');\n          break;\n        case ITALIC: // Ctrl + I\n          this.clickOnFormatTool(quillEditor, celRichTextStore, celRichTextToolbarStore, 'italic', textFormats, 'shortcut');\n          break;\n        case UNDERLINE: // Ctrl + U\n          this.clickOnFormatTool(quillEditor, celRichTextStore, celRichTextToolbarStore, 'underline', textFormats, 'shortcut');\n          break;\n        case UNDO: // Ctrl + Y\n          this.redo(quillEditor, rteFixedValues, celRichTextStore, celRichTextToolbarStore);\n          break;\n        case REDO: // Ctrl + Z\n          this.handleRetainFormat(quillEditor, rteFixedValues, celRichTextStore, celRichTextToolbarStore);\n          break;\n        default:\n          // Prevents entering text inside RTE, when math keypad is open / RTE is in read only mode\n          if (quillEditor.root.getAttribute('contenteditable') === 'false') {\n            event.preventDefault();\n            event.stopPropagation();\n          }\n      }\n    });\n  }\n  textChange(isNewLine, quillEditor, celRichTextStore, celRichTextToolbarStore, rteFixedValues, _source) {\n    const currentCharCount = this.currentCharCount(quillEditor);\n    const currentRteText = this.getContents(quillEditor);\n    const currentRange = this.currentSelection(quillEditor);\n    celRichTextStore.state.textChange = { currentCharCount, currentRteText, source: _source };\n    if (isNewLine) {\n      celRichTextStore.state.selectedRange = currentRange;\n      this.retainEditorFormat(quillEditor, celRichTextToolbarStore, rteFixedValues);\n      this.setCursorPosition(quillEditor, celRichTextStore, 'user');\n      this.toggleUndoRedoStates(quillEditor, celRichTextToolbarStore);\n    }\n    else {\n      this.handleRetainFormat(quillEditor, rteFixedValues, celRichTextStore, celRichTextToolbarStore, currentRange);\n    }\n  }\n  handleRetainFormat(quillEditor, rteFixedValues, celRichTextStore, celRichTextToolbarStore, range) {\n    const { textFormats, listFormats } = rteFixedValues;\n    const currentRange = range ? range : this.currentSelection(quillEditor);\n    const currentFormat = (!currentRange || currentRange.length === 0) ? quillEditor.getFormat() : quillEditor.getFormat(currentRange);\n    celRichTextStore.state.selectedRange = currentRange;\n    if (currentFormat && !(currentFormat['type'] === 'math')) {\n      textFormats.forEach((format) => {\n        const state = !!currentFormat[format];\n        if ((celRichTextToolbarStore.state[format] !== state)) {\n          celRichTextToolbarStore.state[format] = state;\n        }\n      });\n    }\n    const { list } = currentFormat;\n    listFormats.forEach((format) => {\n      const state = !!(list && list === format);\n      if (celRichTextToolbarStore.state[format] !== state) {\n        celRichTextToolbarStore.state[format] = state;\n      }\n    });\n    this.toggleUndoRedoStates(quillEditor, celRichTextToolbarStore);\n  }\n  toggleUndoRedoStates(quillEditor, celRichTextToolbarStore) {\n    const undoPossible = (quillEditor.history.stack.undo.length > 0);\n    const redoPossible = (quillEditor.history.stack.redo.length > 0);\n    celRichTextToolbarStore.state[toolbarItemLabel.undo] = undoPossible;\n    celRichTextToolbarStore.state[toolbarItemLabel.redo] = redoPossible;\n  }\n  isRetainableFocusedIndexFormat(previousRange, currentRange, quillEditor) {\n    let isLastIndex = false;\n    try {\n      const { index: previousFocusedIndex } = previousRange;\n      const { index: currentFocusedIndex } = currentRange;\n      const currentCount = this.currentCharCount(quillEditor);\n      isLastIndex = ((previousFocusedIndex === currentFocusedIndex) && currentFocusedIndex === currentCount && currentCount !== 0);\n    }\n    catch (_a) { }\n    return isLastIndex;\n  }\n  retainEditorFormat(quillEditor, celRichTextToolbarStore, rteFixedValues) {\n    const { textFormats } = rteFixedValues;\n    textFormats.forEach((format) => {\n      quillEditor.format(format, celRichTextToolbarStore.state[format], 'user');\n    });\n  }\n  isMathEmbedExistByIndex(index, quillEditor) {\n    var _a, _b;\n    const content = quillEditor.getContents(index, 1);\n    return !!((_b = (_a = content.ops[0]) === null || _a === void 0 ? void 0 : _a.insert) === null || _b === void 0 ? void 0 : _b.mathEmbed);\n  }\n  getTextByIndex(index, quillEditor) {\n    const content = quillEditor.getContents(index, 1);\n    return content.ops[0] ? content.ops[0].insert : '';\n  }\n}\n// This will be a single global singleton instance for every component within cel-component\nconst celRichTextInstance = new CelRichTextService();\n\nconst celRichTextCss = \"/*!\\n * Quill Editor v1.3.7\\n * https://quilljs.com/\\n * Copyright (c) 2014, Jason Chen\\n * Copyright (c) 2013, salesforce.com\\n */.ql-container{-webkit-box-sizing:border-box;box-sizing:border-box;font-family:Helvetica, Arial, sans-serif;font-size:13px;height:100%;margin:0px;position:relative}.ql-container.ql-disabled .ql-tooltip{visibility:hidden}.ql-container.ql-disabled .ql-editor ul[data-checked]>li::before{pointer-events:none}.ql-clipboard{left:-100000px;height:1px;overflow-y:hidden;position:absolute;top:50%}.ql-clipboard p{margin:0;padding:0}.ql-editor{-webkit-box-sizing:border-box;box-sizing:border-box;line-height:1.42;height:100%;outline:none;overflow-y:auto;padding:12px 15px;-o-tab-size:4;tab-size:4;-moz-tab-size:4;text-align:left;white-space:pre-wrap;word-wrap:break-word}.ql-editor>*{cursor:text}.ql-editor p,.ql-editor ol,.ql-editor ul,.ql-editor pre,.ql-editor blockquote,.ql-editor h1,.ql-editor h2,.ql-editor h3,.ql-editor h4,.ql-editor h5,.ql-editor h6{margin:0;padding:0;counter-reset:list-1 list-2 list-3 list-4 list-5 list-6 list-7 list-8 list-9}.ql-editor ol,.ql-editor ul{padding-left:1.5em}.ql-editor ol>li,.ql-editor ul>li{list-style-type:none}.ql-editor ul>li::before{con