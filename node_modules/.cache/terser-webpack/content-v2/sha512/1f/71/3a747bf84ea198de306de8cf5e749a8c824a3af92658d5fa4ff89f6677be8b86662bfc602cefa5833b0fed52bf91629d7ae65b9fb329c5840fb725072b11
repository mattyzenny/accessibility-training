.find((tools) => {\n      if (tools.textFormat) {\n        return tools;\n      }\n    });\n    const { list } = this.toolbarItemList.find((tools) => {\n      if (tools.list) {\n        return tools;\n      }\n    });\n    return { textFormat, list };\n  }\n  render() {\n    return (Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* h */ \"g\"])(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* H */ \"a\"], { \"data-cel-version\": '9.0.2' }, this.rteMode === 'viewer' ? (Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* h */ \"g\"])(\"div\", { class: \"sc-quill-cel-rich-text-editor-container\" }, Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* h */ \"g\"])(\"div\", { class: \"sc-quill-cel-rich-text ql-container ql-snow\" }, Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* h */ \"g\"])(\"div\", { class: \"ql-editor\", tabindex: \"0\", \"aria-label\": this.reviewModeAriaLabel, \"aria-readonly\": \"true\", role: \"textbox\", innerHTML: this.reviewContent() })))) :\n      Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* h */ \"g\"])(\"div\", { class: \"sc-quill-cel-rich-text-editor-container\" }, Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* h */ \"g\"])(\"div\", { role: \"application\", \"aria-label\": this.rteAriaLabel }, this.editorTitle ? Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* h */ \"g\"])(\"span\", { class: \"sc-quill-cel-rich-text-editor-title\" }, \" \", this.editorTitle) : null, Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* h */ \"g\"])(\"div\", null, Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* h */ \"g\"])(this.cel_rich_text_toolbar, { celRteEditorStore: this.editorStore$, celRteToolbarStore: this.toolbarStore$, \"data-cel-icon-base-url\": this.celIconBaseUrl, \"data-enable-math-editor\": this.enableMathEditor, \"data-toolbar-aria-label\": this.toolbarAriaLabel })), Object(_index_9a8c46b4_js__WEBPACK_IMPORTED_MODULE_0__[/* h */ \"g\"])(\"div\", { ref: (el) => (th